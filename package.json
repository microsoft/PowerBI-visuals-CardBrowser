{
  "name": "powerbi-visuals-cardbrowser",
  "version": "1.0.6",
  "description": "Card Browser, a custom visual for PowerBI",
  "main": "src/index.js",
  "scripts": {
    "preinstall:prereq": "npm install targz mv --save-dev",
    "preinstall:submodules": "node bin/downloadPrivateSubmodules",
    "preinstall:dependencies": "node bin/installSubmoduleDeps",
    "preinstall": "npm run preinstall:prereq && npm run preinstall:submodules && npm run preinstall:dependencies",
    "install-certificate": "node bin/openCert",
    "start": "npm run dev",
    "tdd": "npm-run-all --parallel dev test:tdd",
    "dev": "node bin/startDev",
    "package": "node bin/packageVisual",
    "postinstall": "node bin/createSymLink",
    "test": "karma start --color | node bin/filterErrors",
    "test:tdd": "karma start --color --tdd | node bin/filterErrors",
    "lint": "tslint --project ./tsconfig.json src/**/*.ts",
    "clean": "rm -rf node_modules .tmp"
  },
  "config": {},
  "keywords": [],
  "author": "Uncharted Software Inc.",
  "license": "Apache-2.0",
  "privacyTerms": "https://privacy.microsoft.com/en-US/privacystatement/",
  "privateSubmodules": {
    "@uncharted/cards": "0.12.1"
  },
  "devDependencies": {
    "@types/jquery": "^3.3.0",
    "@types/lodash": "^4.14.101",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.1",
    "@types/requirejs": "^2.1.31",
    "@types/sinon": "^4.1.3",
    "@types/sinon-chai": "^2.7.29",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "chokidar": "^2.0.0",
    "clean-css": "^4.1.9",
    "connect": "^3.6.5",
    "eslint": "^4.17.0",
    "eslint-loader": "^1.9.0",
    "handlebars": "^4.0.11",
    "handlebars-loader": "^1.6.0",
    "jest": "^22.4.3",
    "jquery": "^3.3.1",
    "json": "^9.0.6",
    "karma": "^2.0.0",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jquery": "^0.2.2",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sinon-chai": "^1.3.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-typescript-preprocessor": "^0.3.1",
    "karma-webpack": "^2.0.9",
    "memory-fs": "^0.4.1",
    "mkdirp": "^0.5.1",
    "mocha": "^5.0.0",
    "mv": "^2.1.1",
    "node-sass": "^4.7.2",
    "node-zip": "^1.1.1",
    "npm-run-all": "^4.1.2",
    "onchange": "^3.3.0",
    "powerbi-visuals": "^0.6.0",
    "serve-static": "^1.13.2",
    "sinon": "^4.2.2",
    "sinon-chai": "^2.14.0",
    "targz": "^1.0.1",
    "ts-jest": "^22.4.2",
    "ts-loader": "^3.5.0",
    "tslint": "^5.9.1",
    "tslint-loader": "^3.5.3",
    "tslint-microsoft-contrib": "^5.0.3",
    "typescript": "^2.7.2",
    "webpack": "^3.11.0"
  },
  "dependencies": {
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.5",
    "moment": "^2.20.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
