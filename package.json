{
  "name": "powerbi-visuals-cardbrowser",
  "version": "1.1.2",
  "description": "Card Browser, a custom visual for PowerBI",
  "main": "src/index.js",
  "scripts": {
    "install-certificate": "node bin/openCert",
    "postinstall": "npm run install-private-submodule",
    "install-private-submodule": "node bin/downloadPrivateSubmodules && node bin/installSubmoduleDeps",
    "start": "yarn run dev",
    "tdd": "npm-run-all --parallel dev test:watch",
    "dev": "node bin/startDev",
    "package": "node bin/packageVisual",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "tslint --project ./tsconfig.json src/**/*.ts",
    "clean": "rm -rf node_modules .tmp"
  },
  "config": {},
  "keywords": [],
  "author": "Uncharted Software Inc.",
  "license": "MIT",
  "privacyTerms": "https://privacy.microsoft.com/en-US/privacystatement/",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/PowerBI-visuals-CardBrowser"
  },
  "privateSubmodules": {
    "@uncharted/cards": "0.13.5"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/jquery": "^3.3.0",
    "@types/lodash-es": "^4.17.0",
    "@types/moment": "^2.13.0",
    "@types/node": "^9.4.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "chokidar": "^2.0.0",
    "clean-css": "^4.1.9",
    "connect": "^3.6.5",
    "eslint": "^4.17.0",
    "eslint-loader": "^1.9.0",
    "handlebars": "^4.0.11",
    "handlebars-loader": "^1.6.0",
    "jest": "^22.4.3",
    "jquery": "^3.3.1",
    "json": "^9.0.6",
    "memory-fs": "^0.4.1",
    "mkdirp": "^0.5.1",
    "mv": "^2.1.1",
    "node-sass": "^4.7.2",
    "node-zip": "^1.1.1",
    "npm-run-all": "^4.1.2",
    "onchange": "^3.3.0",
    "powerbi-visuals": "^0.6.0",
    "serve-static": "^1.13.2",
    "sinon": "^4.2.2",
    "sinon-chai": "^2.14.0",
    "targz": "^1.0.1",
    "ts-jest": "^22.4.2",
    "ts-loader": "^3.5.0",
    "tslint": "^5.9.1",
    "tslint-loader": "^3.5.3",
    "tslint-microsoft-contrib": "^5.0.3",
    "typescript": "^2.7.2",
    "webpack": "^3.11.0"
  },
  "dependencies": {
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.4",
    "lodash-es": "^4.17.8",
    "moment": "^2.20.1"
  },
  "engines": {
    "node": ">=8"
  },
  "jest": {
    "transform": {
      "^.+\\.(j|t)sx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!lodash-es/.*)"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
